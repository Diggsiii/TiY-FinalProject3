

<div class="jumbotron center">

    <h1>Welcome to Zoom-it <i class="fa fa-smile-o"></i></h1>
    <div class="indexP">
        <p>Glad you stopped by! If you haven't done so already, please sign in below.</p>
    </div>
    <% if user_signed_in? %>
    <%= link_to 'Logout', destroy_user_session_path, method: :delete, class: 'btn btn-primary' %>
    <%= link_to 'Edit Profile', edit_user_registration_path, class: 'btn btn-default' %>
    <%= link_to 'Zooms', zooms_path, class: 'btn btn-default' %>
<% else %>
    <%= link_to "Login", new_user_session_path, class: 'btn btn-default' %>
    <%= link_to "Sign up", new_user_registration_path, class: 'btn btn-primary' %>
    <% end %>
